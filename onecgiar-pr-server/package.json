{
  "name": "onecgiar-pr-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --verbose false --config ./test/jest-e2e.json --detectOpenHandles",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:empty": "cross-replace npm run typeorm migration:create ./src/migrations/${npm_config_name}",
    "migration:generate": "cross-replace npm run typeorm migration:generate -- ./src/migrations/${npm_config_name} -d ./src/config/orm.config.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d ./src/config/orm.config.ts",
    "migration:run": "npm run typeorm migration:run -- -d ./src/config/orm.config.ts",
    "lambda:test": "sls offline start",
    "lambda:deploy": "sls deploy -v"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.804.0",
    "@nestjs/axios": "^4.0.0",
    "@nestjs/common": "^11.0.4",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.4",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/microservices": "^11.0.4",
    "@nestjs/platform-express": "^11.1.5",
    "@nestjs/schedule": "^5.0.0",
    "@nestjs/swagger": "^11.0.2",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "activedirectory": "^0.7.2",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1692.0",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-replace": "^0.2.0",
    "dotenv": "^16.4.7",
    "dynamoose": "^4.0.3",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "ldapts": "^8.0.5",
    "mysql2": "^3.12.0",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-jwt": "^4.0.1",
    "pusher": "^5.2.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "uuid": "^11.0.5",
    "webpack": "^5.97.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/passport": "^11.0.4",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.4",
    "@types/aws-lambda": "^8.10.147",
    "@types/aws-serverless-express": "^3.3.10",
    "@types/cron": "^2.4.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.1",
    "prettier": "^3.4.2",
    "serverless-offline": "^13.9.0",
    "serverless-plugin-optimize": "^4.2.1-rc.1",
    "serverless-plugin-typescript": "^2.1.5",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.7.3"
  },
  "lint-staged": {
    "**/*.ts": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/src/migrations/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/dist/",
      "/src/migrations/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 5,
        "functions": 30,
        "lines": 35,
        "statements": 40
      }
    },
    "coverageReporters": [
      "text",
      "cobertura",
      "lcov"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1"
    }
  }
}
